{
  "name": "github-user-search",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "config": "npx ts-node ./src/config/config.ts",
    "start": "npm run config && ng serve",
    "build": "ng build",
    "build:prod": "npm run config && ng build --sourceMap=false --base-href /github-user-search/",
